#include <stdio.h>
#include <locale.h>

int main()
{
setlocale(LC_ALL, "Rus");
float A1, A2, B1, B2, C1, C2, Delta, Delta1, Delta2, x, y;
printf("Введите значения системы неравенств\n");
printf("Введите значение А1 : ");
scanf("%f", &A1);
printf("Введите значение B1 : ");
scanf("%f", &B1);
