#include <stdio.h>
#include <locale.h>
#include <math.h>
float fact2(int n){
 int temp=1;
 for (;n>0;n-=2) temp*=n;
 return temp;
}
int main(void)
{
 setlocale(LC_ALL, "Russian");
 int i, n;
 for (i=1; i<=5; ++i){
 printf("Введите N:");
 scanf("%i", &n);
 printf("Функция fact2 : %f\n",fact2(n));
 }
 return 0;
}
