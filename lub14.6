#include <stdio.h>
#include <locale.h>
#include <math.h>
int main()
{
    setlocale(LC_ALL, "Russian");
    int N;
	printf("Введите целое число N (N>1)");
	scanf("%d",&N);

	if (N == 0)
	{
		printf("Порядковый номер числа Фибоначчи N = ");
		printf("1");
	}
	else if (N == 1)
	{
		 printf("Порядковый номер числа Фибоначчи N = ");
		 printf("2,3");
	}
	else
	{
		int f1 = 1;
		int f2 = 1;
		int f = 0;
		int K = 2;

		while (f < N)
		{
			K++;
			f = f2 + f1;
			f2 = f1;
			f1 = f;
		}

		printf("Порядковый номер числа Фибоначчи N = %d ", K + 1);

	}

}
